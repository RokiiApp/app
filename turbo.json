{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "package:test": {},
        "package:type-check": {},
        "package:build": {
            "dependsOn": [
                "package:type-check",
                "package:test"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "app:type-check": {
            "dependsOn": [
                "^package:build"
            ]
        },
        "app:build": {
            "dependsOn": [
                "app:type-check"
            ]
        }
    }
}
